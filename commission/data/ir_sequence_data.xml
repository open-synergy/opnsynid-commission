<odoo>
<data noupdate="1">
<record id="sequence_commission" model="ir.sequence">
    <field name="name">Default Commission Sequence</field>
    <field name="code">commission.commision</field>
    <field name="prefix">COM/%(range_year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
</data>
</odoo>
